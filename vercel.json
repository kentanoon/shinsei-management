{
  "version": 2,
  "name": "shinsei-management",
  "buildCommand": "cd frontend && npm ci && DISABLE_ESLINT_PLUGIN=true CI=false npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd frontend && npm ci",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_DEMO_MODE": "true",
    "REACT_APP_API_URL": "https://demo-api.example.com/api/v1",
    "REACT_APP_SITE_URL": "https://shinsei-management.vercel.app",
    "CI": "false",
    "DISABLE_ESLINT_PLUGIN": "true",
    "TSC_COMPILE_ON_ERROR": "true",
    "ESLINT_NO_DEV_ERRORS": "true"
  }
}